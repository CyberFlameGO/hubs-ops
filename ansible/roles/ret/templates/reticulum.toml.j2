[phx]
{% if phoenix_port is defined %}
port = {{ phoenix_port }}
{% else %}
port = 4000
{% endif %}
secret_key = "{{ phoenix_secret_key }}"
url_host = "{{ phoenix_url_host }}"
{% if phoenix_url_port is defined %}
url_port = "{{ phoenix_url_port }}"
{% endif %}
